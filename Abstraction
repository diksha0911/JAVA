abstract class figure
{
abstract void show();
}
abstract class A
{
abstract void callme();
void callmetoo()
{
System.out.println("This is a concrete method");
}}
class B extends A
{
void callme()
{
System.out.println("B's implementation of callme");
}}
class Abstractdemo
{
public static void main(String arg[])
{
B b=new B();
b.callme();
b.callmetoo();
}}
