import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	    int a,b,c;
	    Scanner s=new Scanner(System.in);
	    System.out.println("Enter value of a & b:");
	    a=s.nextInt();
	    b=s.nextInt();
	    c=a+b;
	    System.out.println("Sum of "+a+" and "+b+" is "+c);
	}
}
